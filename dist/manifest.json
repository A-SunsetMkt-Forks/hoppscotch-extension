{
  "name": "Postwoman extension",
  "version": "0.1",
  "description": "Proxies Postwoman requests through extension to override CORS",
  "background": {
    "scripts": [ "index.js" ],
    "persistent": false 
  },
  "permissions": [
    "https://*/",
    "http://*/"
  ],
  "externally_connectable": {
    "matches": ["http://localhost:3000/*"]
  },
  "manifest_version": 2
}
